{
  "name": "Python",
  "dockerComposeFile": "./docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "remoteUser": "vscode",
  "initializeCommand": "mkdir -p .devcontainer/dynamodb-data/",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "charliermarsh.ruff",
        "tamasfe.even-better-toml"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/aws-cli:latest": {}
  }
}